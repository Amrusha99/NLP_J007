      "source": [
        "def nth_prime(n):\n",
        "  num_list=[]\n",
        "  k=int(2)\n",
        "  while (len(num_list)<n):\n",
        "    count=0\n",
        "    for i in range (1,int(k**(1/2)+1)):\n",
        "      if(k%i==0):\n",
        "        count+=1\n",
        "    if(count==1):\n",
        "      num_list.append(k)\n",
        "    k+=1\n",
        "  print(num_list[-1])"
      ],
